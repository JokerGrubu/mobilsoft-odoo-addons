<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Luca Export User Group -->
    <record id="group_luca_export_user" model="res.groups">
        <field name="name">Luca Export Kullanıcısı</field>
        <field name="implied_ids" eval="[(4, ref('account.group_account_user'))]"/>
    </record>

    <!-- Luca Export Manager Group -->
    <record id="group_luca_export_manager" model="res.groups">
        <field name="name">Luca Export Yöneticisi</field>
        <field name="implied_ids" eval="[(4, ref('group_luca_export_user'))]"/>
    </record>

</odoo>
