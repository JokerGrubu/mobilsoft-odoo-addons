<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Demo Garanti BBVA Connector -->
        <record id="demo_garantibbva_connector" model="bank.connector.garantibbva">
            <field name="name">Garanti BBVA Sandbox</field>
            <field name="bank_id" ref="bank_garantibbva" />
            <field name="client_id">sandbox_demo_client_id</field>
            <field name="client_secret">sandbox_demo_secret</field>
            <field name="sandbox_mode" eval="True" />
            <field name="auto_sync_enabled" eval="False" />
            <field name="state">draft</field>
        </record>

    </data>
</odoo>
